<%= button_to "Log out", 
    destroy_user_session_path, 
    method: :delete, 
    class: "nav-link" 
%>
<%= button_to "Log in", 
    new_user_session_path, 
    method: :get, 
    class: "nav-link" 
%>
<table>
    <tr>
        <th>Author</th>
        <th>Content</th>
    </tr>
    <% @posts.each do |post| %>
    <tr>
        <td><%= user_signed_in? ? User.find(post.user_id).username : "Anon-chan" %></td>
        <td><%= post.content %></td>
    </tr>
    <% end %>
</table>